
<div class="twelve columns partyorder">
<h2>
<%= form_for @orders do |f| %>
<%= f.label :party_id %>
<%= f.select( :party_id, options_for_select(@parties.map {|party| [party.reservation_name, party.id ]}) ) %>

<%= f.label :item %>
<%= f.select( :item_id, options_for_select(@item.map {|item| [item.name, item.id ]}) ) %>
<br>
<%= f.submit %>
<% end %>
</h2>
</div>
